	final public class RGB 
	{
		public var r:Number = 0;
        public var g:Number = 0;
        public var b:Number = 0;
		public var a:Number = 1;
		public function RGB(R:Number = 0, G:Number = 0, B:Number = 0, A:Number = 1)
		{
			this.r = R;
            this.g = G;
            this.b = B;
			this.a = A;
		}
		
	}

}